


CREATE TABLE
    dtra.public.activity
    (
    	activity_id bigint,
        activity_name VARCHAR(100),
        activity_norm_hours INTEGER,
        activity_lead_time INTEGER,
        activity_intensity INTEGER,
        time_dependency_type VARCHAR(2),
        lag_time bigint,
        time_dependent_activity bigint,
        engineer_dependency INTEGER,
        engineer_dependent_activity INTEGER,
        competence_profiles INTEGER[],
        priority INTEGER,
        preferred_engineers VARCHAR[],
        required_engnineers VARCHAR[],
        delivery_unit INTEGER,
        unit INTEGER,
        early_start DATE,
        late_start DATE,
        due_date DATE,
        PRIMARY KEY (activity_id)
    )
    
    
    
CREATE TABLE
    dtra.public.engineer
    (
    	engineer_id bigint,
        status SMALLINT,
        corporate_id INTEGER,
        last_name VARCHAR,
        first_name VARCHAR,
        know_as VARCHAR,
        email_address VARCHAR,
        country INTEGER,
        primary_org INTEGER,
        company_code INTEGER,
        org_unit_short VARCHAR,
        name_of_superior VARCHAR,
        pers_no_of_superior INTEGER,
        id_of_superior VARCHAR,
        personnel_subarea VARCHAR,
        cost_center bigint,
        unit INTEGER,
        competences INTEGER[],
        engineer_profile bigint,
        index_of_enginner_expertise INTEGER,
        calendar INTEGER,
        PRIMARY KEY (engineer_id)
    )
    
    
CREATE TABLE
    dtra.public.work_package
    (
    	wp_id VARCHAR(10),
        wp_name VARCHAR(100),
        wp_category INTEGER,
        wp_norm_hours INTEGER,
        wp_lead_time INTEGER,
        wp_release VARCHAR(10),
        wp_status INTEGER,
        ordering_origin bigint,
        delivery_unit INTEGER,
        PRIMARY KEY (wp_id)
    )